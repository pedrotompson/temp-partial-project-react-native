{
  "expo": {
    "name": "StartupProject",
    "slug": "startupproject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/visual/icon.png",
    "splash": {
      "image": "./src/assets/visual/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#050934"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
