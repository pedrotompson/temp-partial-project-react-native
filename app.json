    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/visual/adaptive-icon.png",
        "backgroundColor": "#050934"
      }
    },
    "web": {
      "favicon": "./src/assets/visual/favicon.png"
    },
    "description": "Projeto desenvolvido para Linqtree",
    "githubUrl": "https://github.com/pedrotompson/startup-project-mobile"
  }
}
